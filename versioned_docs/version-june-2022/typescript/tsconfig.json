{
  "compilerOptions": {
    "moduleResolution": "node",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noResolve": true,
    "allowUnusedLabels": true,
    "skipLibCheck": true,
    "noEmitOnError": true,
    "strict": false,
    "removeComments": false,
    "module": "ESNext",
    "target": "ESNext",
    "alwaysStrict": false
  }
}
