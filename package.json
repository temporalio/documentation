{
  "name": "temporal-documentation",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "assemble": "npm-run-all --serial assemble-local format",
    "assemble-local": "npm-run-all --parallel -r worker assemble-cmd",
    "assemble-cloud": "npm-run-all --parallel -r worker assemble-cloud-cmd",
    "assemble-cmd": "./assemble.js",
    "assemble-cloud-cmd": "./assemble.js --cloud",
    "build": "docusaurus build",
    "clear": "docusaurus clear",
    "deploy": "docusaurus deploy",
    "docusaurus": "docusaurus",
    "format": "dprint fmt",
    "gen": "npm-run-all --serial snipsync assemble-local format build",
    "gen-cloud": "npm-run-all --serial snipsync assemble-cloud format build",
    "check-links": "hyperlink build/index.html --recursive --check-anchors",
    "lint": "vale docs-src/**/*.md",
    "lint:go": "vale docs-src/go/*.md",
    "lint:java": "vale docs-src/java/*.md",
    "lint:py": "vale docs-src/python/*.md",
    "lint:ts": "vale docs-src/typescript/*.md",
    "serve": "docusaurus serve",
    "start": "docusaurus start",
    "snipsync": "snipsync",
    "swizzle": "docusaurus swizzle",
    "worker": "cd assembly && yarn worker && cd ..",
    "cloud-worker": "cd assembly && ./worker.js --cloud && cd ..",
    "write-heading-ids": "docusaurus write-heading-ids",
    "write-translations": "docusaurus write-translations"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@docusaurus/core": "^2.4.3",
    "@docusaurus/preset-classic": "^2.4.3",
    "@docusaurus/theme-mermaid": "^2.4.3",
    "@mdx-js/react": "^1.5.8",
    "beautiful-react-diagrams": "^0.5.1",
    "chart.js": "3.7.1",
    "clsx": "^1.1.1",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-player": "^2.6.0",
    "remark-typescript-tools": "1.0.9",
    "tar-fs": "^2.1.1",
    "typescript": "4.x",
    "uuid": "^8.3.2",
    "webpack-font-preload-plugin": "^1.5.0"
  },
  "devDependencies": {
    "dprint": "^0.40.0",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.23.2",
    "fs-extra": "^10.1.0",
    "gray-matter": "^4.0.3",
    "hyperlink": "^5.0.4",
    "readdirp": "^3.6.0",
    "snipsync": "1.7.0",
    "temporalio": "^1.1.0"
  }
}
